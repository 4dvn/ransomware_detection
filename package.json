	"engines": {
		"node": ">=10.0.0"
	},
	"dependencies": {
		"vue": "^2.6.10"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-syntax-dynamic-import": "^7.2.0",
		"@babel/preset-env": "^7.4.5",
		"@vue/test-utils": "^1.0.0-beta.29",
