		"vue-style-loader": "^4.1.1",
		"vue-template-compiler": "^2.6.10",
		"webpack": "^4.32.2",
		"webpack-cli": "^3.3.2",
    "webpack-merge": "^4.2.1",
    "cross-env": "^5.2.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"vue"
		],
		"moduleNameMapper": {
			"^@/(.*)$": "<rootDir>/src/$1"
		},
		"transform": {
			"^.+\\.js$": "<rootDir>/node_modules/babel-jest",
			".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
		},
		"snapshotSerializers": [
			"<rootDir>/node_modules/jest-serializer-vue"
		]
	}
